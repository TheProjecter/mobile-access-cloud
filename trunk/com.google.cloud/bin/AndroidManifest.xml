<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.google.cloud.ui"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:maxSdkVersion="16"
        android:minSdkVersion="11" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.google.cloud.ui.CloudApplication"
        android:allowBackup="true"
        android:icon="@drawable/cloud_app"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.google.cloud.ui.activities.StartMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.cloud.activities.ManageNetworks" >
        </activity>
        <activity android:name="com.google.cloud.activities.ManageStorages" >
        </activity>
        <activity android:name="com.google.cloud.activities.NetworkActivity" />
    </application>

</manifest>